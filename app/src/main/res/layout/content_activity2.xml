<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.sakshi.offlinesecuritypage1.activity2"
    tools:showIn="@layout/activity_activity2"
    android:background="#a2a7ab">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="WhatsApp"
        android:id="@+id/whatsApp_textView4"
        android:layout_marginTop="89dp"
        android:textColor="#020202"
        android:textSize="@dimen/abc_dropdownitem_icon_width"
        android:layout_below="@+id/textView5"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Gallery"
        android:id="@+id/textView4"
        android:layout_marginTop="59dp"
        android:textColor="#020202"
        android:textSize="@dimen/abc_dropdownitem_icon_width"
        android:layout_below="@+id/whatsApp_textView4"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Hike"
        android:id="@+id/hike_textView5"
        android:layout_marginTop="66dp"
        android:textColor="#020202"
        android:textSize="@dimen/abc_dropdownitem_icon_width"
        android:layout_below="@+id/textView4"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/whatsApp_checkBox"
        android:layout_alignTop="@+id/whatsApp_textView4"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:checked="false" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/gallery_checkBox"
        android:checked="false"
        android:layout_alignTop="@+id/textView4"
        android:layout_alignLeft="@+id/whatsApp_checkBox"
        android:layout_alignStart="@+id/whatsApp_checkBox" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/hike_checkBox2"
        android:checked="false"
        android:layout_alignTop="@+id/hike_textView5"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="next"
        android:id="@+id/button2"
        android:layout_below="@+id/hike_textView5"
        android:layout_alignRight="@+id/hike_checkBox2"
        android:layout_alignEnd="@+id/hike_checkBox2"
        android:layout_marginTop="53dp"
        android:background="#d3d5d7"
        android:textColor="#101010" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="INSTALLED...."
        android:id="@+id/textView5"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

</RelativeLayout>
